
 # tuple are like list , it just the they are immutable , or cannot be chaged after delecration


 #creating tuples
empty_tuple =()
print(empty_tuple)
tup1=('hyy','there',3)														
print(tup1)																								

			
#(my god!!!      the output TT::()
#('hyy', 'there', 3) )

print("  ")



#concentration of tuples

tup1 = ('hyy','there')
tup2 = (1,2)
print(tup1 + tup2)


print("  ")

#nested tupels 			"when tuples contsin more then one tupels"

t1=('hyee','there','!!!')
t2=(1,2,3,4,5,6)
t3=('A','N','C','H','A','L')
t4=(t1+t2+t3)
print(t4)


print("  ")

#repetition in tuples

tup1 = ('hyy','there')*5
print(tup1)


print("  ")


##inputs can't be changed

#length of tuples
t2=(1,2,3,4,5,6)
print(len(t2))


print("  ")

#convert list into tuples

list=[1,2,3,4,5]
temp = tuple(list)
print (temp)













































































































