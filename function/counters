  
#unordered collection of elements where collectives count are stored as a dictionary
# counter can be classified in three ways 

from collections import Counter

#with sequence of items
print(Counter(['B','E','F','E','H','E','B','V']))

#with dictionary
print(({'a':3,'y':6,'s':7}))

#with keyword arguments
print(Counter(A=5,R=5,H=6))

print(" ")

#update function  					:print dictionary

from collections import Counter
coun = Counter()
print(coun)

coun.update([1,2,3,4,4,4,4,2,1,2,3])
print(coun)

coun.update([1,3,6])
print(coun)

print(" ")

#python program that counts can be 0 or negative

from collections import Counter
c1= Counter(A=4,F=4,H=7)
c2=Counter(A=1,F=7,H=9)

c1.subtract(c2)
print(c1)










































































































































