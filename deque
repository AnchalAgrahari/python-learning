import collections

de = collections.deque([1,2,3])						#instializing deque
print()
print(de)
de.append(4)
print()
print("modified deque :",de)
print()
de.appendleft(0)
print("2nd modified deque :", de)
print()
de.pop()
print("The deque after deleting from right :",end=" ")
print(de)
print()
print(de.index(0))
print()
de.insert(4,4)
de.insert(5,5)
de.insert(6,6)
print("The deque after insertation :", de)
print()
print("The count of 3 in deque is :", de.count(3))
print()